<body class="animsition">
	
	<!-- Header -->
	<header class="header-v4">
		<!-- Header desktop -->
		<div class="container-menu-desktop trans-03">
			<div class="wrap-menu-desktop how-shadow1">
				<nav class="limiter-menu-desktop container header2-nav">
					
					<!-- Logo desktop -->		
					<a href="/" class="logo">
						<img src="images/icons/logothumbnailblack.png" alt="IMG-LOGO">
					</a>

					<!-- Menu desktop -->
					<div class="menu-desktop">
						<ul class="main-menu">
							<li>
								<a href="/">Home</a>
								
							</li>

							<li class="<%= currentpage === 'product' ? 'active-menu' : '' %>">
								<a href="/products">Product</a>
							  </li>
						  
							  <li class="<%= currentpage === 'about' ? 'active-menu' : '' %>">
								<a href="/about">About</a>
							  </li>
						  
							  <li class="<%= currentpage === 'contact' ? 'active-menu' : '' %>">
								<a href="/contact">Contact</a>
							  </li>
						</ul>
					</div>	
				</nav>
			</div>	
		</div>

		<!-- Header Mobile -->
		<div class="wrap-header-mobile">
			<!-- Logo moblie -->		
			<div class="logo-mobile">
				<a href="/"><img src="images/icons/logothumbnailblack.png" alt="IMG-LOGO"></a>
			</div>

			<!-- Button show menu -->
			<div class="btn-show-menu-mobile hamburger hamburger--squeeze">
				<span class="hamburger-box">
					<span class="hamburger-inner"></span>
				</span>
			</div>
		</div>


		<!-- Menu Mobile -->
		<div class="menu-mobile">
			

			<ul class="main-menu-m">
				<li class="active-menu">
					<a href="/">Home</a>
					
					<span class="arrow-main-menu-m">
						<i class="fa fa-angle-right" aria-hidden="true"></i>
					</span>
				</li>

				<li>
					<a href="/products">Product</a>
				</li>

				<li>
					<a href="/about">About</a>
				</li>

				<li>
					<a href="/contact">Contact</a>
				</li>
			</ul>
		</div>
		<!-- Modal Search -->
	</header>